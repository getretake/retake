{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Retake",
  "logo": {
    "dark": "/logo/primary.svg",
    "light": "/logo/primary.svg"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#34d399",
    "light": "#34d399",
    "dark": "#34d399"
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
