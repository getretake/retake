    },
    {
      "group": "Deployment",
      "pages": ["deployment/local", "deployment/kubernetes"]
    },
    {
      "group": "Company",
      "pages": ["company/roadmap"]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/getretake/retake"
  },
  "backgroundImage": "/background.png"
